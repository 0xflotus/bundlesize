{
  "name": "bundlesize",
  "version": "0.0.1",
  "description": "Keep your library size in check",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "test": "node index"
  },
  "keywords": [
    "library",
    "size",
    "check",
    "build",
    "threshold"
  ],
  "files": [
    "index.js"
  ],
  "author": "siddharthkp",
  "license": "MIT",
  "dependencies": {
    "bytes": "2.5.0",
    "gzip-size": "3.0.0",
    "prettycli": "1.3.0",
    "read-pkg-up": "2.0.0"
  },
  "bundlesize": [
    {
      "path": "./index.js",
      "threshold": "100B"
    },
    {
      "path": "./index.js",
      "threshold": "1KB"
    }
  ]
}
